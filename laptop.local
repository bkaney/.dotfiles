#!/bin/sh

brew bundle --file=- <<EOF
brew "ack"
brew "neovim"

brew "coreutils"
brew "findutils"
brew "gnu-sed"
brew "tree"
brew "nvm"
brew "python"
EOF

brew cask install "iterm2"
brew cask install "google-chrome"
brew cask install "fork"
brew cask install "slack"
brew cask install "authy-desktop"

fancy_echo "Cleaning up old Homebrew formulae ..."
brew cleanup
brew cask cleanup
